# Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=sqlite:///./git_alpha.db

# JWT Secret Key (generate a random string)
SECRET_KEY=your-secret-key-here-change-this

# Groq API Configuration (FREE!)
GROQ_API_KEY=your-groq-api-key-here

# Optional: Model Configuration
GROQ_MODEL=llama-3.3-70b-versatile

# Optional: Agent Configuration
AGENT_DEBUG_MODE=true
AGENT_MARKDOWN=true

# News Hub Configuration
# Get your free API key from: https://newsapi.org/
NEWS_API_KEY=689eb59942f34d7d81b72dc82e3efa07

# OAuth Configuration (Required for Google/GitHub Login)
# Google OAuth - Get credentials from: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# GitHub OAuth - Get credentials from: https://github.com/settings/developers
GITHUB_CLIENT_ID=your-github-client-id-here
GITHUB_CLIENT_SECRET=your-github-client-secret-here

# Session Configuration
SESSION_REMEMBER_ME_DAYS=30
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Angel One API Configuration (for live market data)
# Get your API credentials from: https://smartapi.angelbroking.com/
ANGEL_API_KEY=your-angel-api-key-here
ANGEL_CLIENT_ID=your-client-id-here
ANGEL_PASSWORD=your-password-here
ANGEL_TOTP_KEY=your-totp-secret-key-here
